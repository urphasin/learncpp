{
	"version": "2.0.0",
	"tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: Custom GCC CONFIG",
      "command": "/usr/bin/g++",
      "args": [
        "-std=c++23",
        "${file}",
        "-o",
        "${fileDirname}/a"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "compiler: /usr/bin/g++"
    }
  ]
}